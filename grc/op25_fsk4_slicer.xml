<block>
  <name>Fsk4 slicer</name>
  <key>op25_fsk4_slicer</key>
  <category>OP25</category>
  <import>import op25</import>
  <make>op25.fsk4_slicer($slice_levels)</make>
  <param>
    <name>Slice_levels</name>
    <key>slice_levels</key>
    <value>-3, -1, 1, 3</value>
    <type>real_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
    <nports>MAX_IN</nports>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
    <nports>MAX_OUT</nports>
  </source>
</block>
